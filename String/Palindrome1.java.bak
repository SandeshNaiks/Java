class Pal
{
	static String pal(String str)
	{
		int num=str.length();
		String rev="";
		for (int i=num-1;i>=0;i-- )
		{
			rev=rev+str.charAt(i);
		}
			if(str.equals(rev))
			{
				return ("The given string is Palindrome");
			}
			else
			{
				return ("The given string is not palindrome");
		}
	}
}
